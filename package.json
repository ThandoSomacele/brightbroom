{
  "name": "brightbroom",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && vite build",
    "db:generate": "drizzle-kit generate --schema=./src/lib/server/db/schema.ts --out=./drizzle/migrations --dialect=postgresql",
    "db:migrate:dev": "NODE_ENV=development tsx ./scripts/apply-migrations.ts",
    "db:migrate:prod": "NODE_ENV=production tsx ./scripts/apply-migrations.ts",
    "db:deploy": "./netlify/scripts/deploy-db.sh",
    "db:rollback": "drizzle-kit rollback",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "db:start": "docker compose up -d",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "vite-node src/commands/seed.js",
    "db:seed-services": "tsx scripts/seed-services.ts",
    "db:seed-services:dev": "TARGET_ENV=development tsx scripts/seed-services.ts",
    "db:seed-services:prod": "TARGET_ENV=production tsx scripts/seed-services.ts --confirm",
    "db:seed-services:prod-replace": "TARGET_ENV=production tsx scripts/seed-services.ts --confirm --force-replace",
    "db:seed-services:overwrite": "tsx scripts/seed-services.ts --overwrite",
    "db:backup": "tsx scripts/backup-database.ts",
    "db:reset": "drizzle-kit reset",
    "db:stop": "docker compose down",
    "db:update-services": "tsx scripts/update-service-db.ts",
    "db:update-services:prod": "tsx scripts/update-service-db.ts --production",
    "setup": "node scripts/setup-dirs.js",
    "services:update-order": "tsx scripts/update-service-order.ts",
    "services:convert": "node scripts/convert-service-formats.js",
    "services:json-to-csv": "node scripts/convert-service-formats.js json-to-csv",
    "services:csv-to-json": "node scripts/convert-service-formats.js csv-to-json",
    "test": "vitest",
    "lint": "eslint --ext .js,.ts,.svelte src",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "dev:debug": "DEBUG=* vite dev",
    "build:debug": "DEBUG=* vite build",
    "preview:debug": "DEBUG=* vite preview"
  },
  "dependencies": {
    "@lucia-auth/adapter-drizzle": "^1.1.0",
    "@node-rs/argon2": "^2.0.2",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@sveltejs/adapter-auto": "^4.0.0",
    "@sveltejs/adapter-netlify": "^5.0.0",
    "@sveltejs/kit": "^2.20.2",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@testing-library/svelte": "^5.2.7",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "drizzle-kit": "^0.30.2",
    "drizzle-orm": "^0.40.0",
    "eslint": "^9.22.0",
    "eslint-plugin-svelte": "^3.0.3",
    "jsdom": "^26.0.0",
    "lucia": "^3.2.2",
    "lucide-svelte": "^0.479.0",
    "oslo": "^1.2.1",
    "papaparse": "^5.5.2",
    "postcss": "^8.5.3",
    "postgres": "^3.4.5",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "resend": "^4.1.2",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^3.4.9",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.0.0",
    "vite": "^6.2.3",
    "vite-imagetools": "^7.0.5",
    "vite-node": "^3.0.8",
    "vitest": "^3.0.8",
    "zod": "^3.24.2"
  },
  "svelte-check": {
    "detailed": true
  }
}
