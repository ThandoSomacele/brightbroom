{
  "name": "brightbroom",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "db:start": "docker compose up -d",
    "db:stop": "docker compose down",
    "db:migrate": "tsx scripts/db/migrate.ts",
    "db:migrate:dev": "tsx scripts/db/migrate.ts --env=development",
    "db:migrate:prod": "tsx scripts/db/migrate.ts --env=production --confirm",
    "db:generate": "tsx scripts/db/migrate.ts --generate",
    "db:seed": "tsx scripts/db/seed.ts",
    "db:seed:services": "tsx scripts/db/seed.ts services",
    "db:seed:users": "tsx scripts/db/seed.ts users",
    "db:backup": "tsx scripts/db/utils.ts backup",
    "services:update": "tsx scripts/services/update.ts",
    "services:update:sort": "tsx scripts/services/update.ts --sort-order",
    "services:update:details": "tsx scripts/services/update.ts --details",
    "services:convert": "tsx scripts/services/convert.ts",
    "setup": "node scripts/setup.js",
    "test": "vitest",
    "lint": "eslint --ext .js,.ts,.svelte src",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "dev:debug": "DEBUG=* vite dev"
  },
  "dependencies": {
    "@lucia-auth/adapter-drizzle": "^1.1.0",
    "@node-rs/argon2": "^2.0.2",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@sveltejs/adapter-auto": "^4.0.0",
    "@sveltejs/adapter-netlify": "^5.0.0",
    "@sveltejs/kit": "^2.20.2",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@testing-library/svelte": "^5.2.7",
    "@types/mime-types": "^2.1.4",
    "@types/sharp": "^0.31.1",
    "autoprefixer": "^10.4.20",
    "chalk": "^5.4.1",
    "cross-env": "^7.0.3",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "drizzle-kit": "^0.30.2",
    "drizzle-orm": "^0.40.0",
    "eslint": "^9.22.0",
    "eslint-plugin-svelte": "^3.0.3",
    "jsdom": "^26.0.0",
    "lucia": "^3.2.2",
    "lucide-svelte": "^0.479.0",
    "mime-types": "^2.1.35",
    "nanoid": "^5.0.5",
    "oslo": "^1.2.1",
    "papaparse": "^5.5.2",
    "postcss": "^8.5.3",
    "postgres": "^3.4.5",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "resend": "^4.1.2",
    "sharp": "^0.34.1",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^3.4.9",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.0.0",
    "vite": "^6.2.3",
    "vite-imagetools": "^7.0.5",
    "vite-node": "^3.0.8",
    "vitest": "^3.0.8",
    "zod": "^3.24.2"
  },
  "svelte-check": {
    "detailed": true
  }
}
