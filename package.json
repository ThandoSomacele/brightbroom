{
  "name": "brightbroom",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && vite build",
    "preview": "vite preview",
    "analyze": "vite build --mode=analyze",
    "lint": "eslint . --ext .ts,.js,.svelte --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.js,.svelte --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "db:start": "docker compose up -d",
    "db:stop": "docker compose down",
    "db:push": "drizzle-kit push",
    "db:migrate": "drizzle-kit migrate",
    "db:generate": "npx drizzle-kit generate --schema=./src/lib/server/db/schema.ts --out=./drizzle/migrations --dialect=postgresql",
    "db:studio": "drizzle-kit studio",
    "db:reset,": "drizzle-kit reset",
    "db:seed": "tsx scripts/db/seed.ts",
    "db:seed:services": "tsx scripts/db/seed.ts services",
    "db:seed:users": "tsx scripts/db/seed.ts users",
    "db:seed:earnings": "tsx scripts/db/seed-earnings.ts",
    "db:seed:applications": "tsx scripts/db/seed-applications.ts",
    "db:migrate:training": "tsx scripts/db/migrate-training.ts",
    "db:backup": "tsx scripts/db/backup.ts",
    "db:backup:full": "tsx scripts/db/backup.ts --full",
    "db:deduplicate": "tsx scripts/deduplicate-addresses.ts",
    "services:update": "tsx scripts/services/update.ts",
    "services:update:sort": "tsx scripts/services/update.ts --sort-order",
    "services:update:details": "tsx scripts/services/update.ts --details",
    "services:convert": "tsx scripts/services/convert.ts",
    "update:address-coordinates": "tsx scripts/update-address-coordinates.ts",
    "update:cleaner-coordinates": "tsx scripts/update-cleaner-coordinates.ts",
    "deploy:check": "node scripts/deploy/pre-deploy.js",
    "deploy:netlify": "node scripts/deploy/netlify.js",
    "deploy:validate-env": "node scripts/deploy/validate-env.js",
    "setup": "node scripts/setup.js",
    "test": "vitest",
    "dev:debug": "DEBUG=* vite dev"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.797.0",
    "@aws-sdk/s3-request-presigner": "^3.797.0",
    "@lucia-auth/adapter-drizzle": "^1.1.0",
    "@node-rs/argon2": "^2.0.2",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@sveltejs/adapter-auto": "^4.0.0",
    "@sveltejs/adapter-netlify": "^5.2.4",
    "@sveltejs/kit": "^2.49.3",
    "@sveltejs/vite-plugin-svelte": "^5.1.1",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/svelte": "^5.2.7",
    "@types/mime-types": "^2.1.4",
    "@types/sharp": "^0.31.1",
    "autoprefixer": "^10.4.20",
    "chalk": "^5.4.1",
    "cross-env": "^7.0.3",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "drizzle-kit": "^0.30.2",
    "drizzle-orm": "^0.40.0",
    "eslint": "^9.22.0",
    "eslint-plugin-svelte": "^3.0.3",
    "gray-matter": "^4.0.3",
    "jsdom": "^26.0.0",
    "lucia": "^3.2.2",
    "lucide-svelte": "^0.562.0",
    "marked": "^15.0.12",
    "mime-types": "^2.1.35",
    "nanoid": "^5.0.5",
    "node-fetch": "^3.3.2",
    "oslo": "^1.2.1",
    "papaparse": "^5.5.2",
    "postcss": "^8.5.3",
    "postgres": "^3.4.5",
    "posthog-js": "^1.292.0",
    "posthog-node": "^5.11.2",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "resend": "^4.1.2",
    "sharp": "^0.34.1",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^3.4.9",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.0.0",
    "vite": "^6.4.1",
    "vite-imagetools": "^7.0.5",
    "vite-node": "^3.0.8",
    "vitest": "^3.0.8",
    "zod": "^3.24.2"
  },
  "svelte-check": {
    "detailed": true
  },
  "devDependencies": {
    "@types/marked": "^5.0.2",
    "globals": "^16.4.0"
  }
}
